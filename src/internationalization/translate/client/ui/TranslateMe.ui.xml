<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel width="800px" height="600px">
		<g:CaptionPanel captionText="Translate Me" width="836px" height="756px">
			<g:AbsolutePanel width="100%" height="100%">
				<g:at left="57" top="40">
					<g:ListBox visibleItemCount="1" width="616px" height="25px" ui:field="lbLangs"/>
				</g:at>
				<g:at left="57" top="110">
					<g:CaptionPanel captionText="English" width="612px" height="136px">
						<g:TextBox width="98%" height="80%" alignment="CENTER" ui:field="tbEnglish" readOnly="true" textAlignment="ALIGN_LEFT"/>
					</g:CaptionPanel>
				</g:at>
				<g:at left="57" top="291">
					<g:CaptionPanel captionText="Translation in %s" width="612px" height="118px">
						<g:TextBox width="98%" height="80%" ui:field="tbTargetLanguage" alignment="JUSTIFY" textAlignment="ALIGN_LEFT"/>
					</g:CaptionPanel>
				</g:at>
				<g:at left="55" top="449">
					<g:Grid width="618px" height="100px">
					<g:row>
									<g:customCell>
						<g:Button ui:field="btnPrevious">Previous</g:Button>
						</g:customCell>
										<g:customCell>
						<g:Button ui:field="btnSave">Save</g:Button>
						</g:customCell>
										<g:customCell>
						<g:Button ui:field="btnNext">Next</g:Button>
						</g:customCell>
					</g:row>
					</g:Grid>
				</g:at>
			</g:AbsolutePanel>
		</g:CaptionPanel>
	</g:HTMLPanel>
</ui:UiBinder> 